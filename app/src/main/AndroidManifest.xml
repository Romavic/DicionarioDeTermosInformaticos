<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.NFA.DicionarioDeTermosInformaticos">

    <application
        android:allowBackup="true"
        android:icon="@drawable/image_logo_app"
        android:label="@string/app_name"
        android:roundIcon="@drawable/image_logo_app"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.main.Activity_Dictionary_Words"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.Activity_Configuration"
            android:label="@string/title_activity_configuration"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.Activity_Conversion"
            android:label="@string/title_activity_conversion"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.Activity_Quiz"
            android:label="@string/title_activity_quiz"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.Activity_Save_Words"
            android:label="@string/title_activity_save_words"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.receive_data.Dictionary_Words"
            android:label="@string/title_activity_receive_data_dictionary_words"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.receive_data.Edit_Words"
            android:label="@string/title_activity_edit_words"
            android:theme="@style/AppTheme.NoActionBar"/>

    </application>

</manifest>